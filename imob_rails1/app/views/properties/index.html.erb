<h1>IMOVEIS</h1>
<div clas="properties">
	<% @properties.each do |property| %>
	<div class="property">
		<%= image_tag(property.image_url, width:200) %>
		<h2><%= property.title %></h2>
		<p><%= property.description %></p>
		<span><%= number_to_currrency(property.price) %></span>
		<strong><%= property.user.name %></strong>
	</div>
	<% end %>
</div>
